{
  "manifest_version": 2,
  "name": "Reply to Selected",
  "description": "Simple extension to allow quickly replying to all selected recipients",
  "version": "1.0",
  "author": "Marc Loy",
  "browser_specific_settings": {
    "gecko": {
      "id": "m.dev@loyinc.com",
      "strict_min_version": "91.0"
    }
  },
  "background": {
    "page": "background.html"
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "icons": {
    "64": "images/rts-64.png",
    "32": "images/rts-32.png",
    "16": "images/rts-16.png"
  },
  "permissions": [
    "accountsRead",
    "compose",
    "menus",
    "messagesRead",
    "storage"
  ]
}